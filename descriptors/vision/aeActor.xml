<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
<frameworkImplementation>org.apache.uima.java</frameworkImplementation>
<primitive>true</primitive>
<annotatorImplementationName>org.tom.annotators.ActorAnnot</annotatorImplementationName>
<analysisEngineMetaData>
<name>aeActor</name>
<description>Finds Actors in diferents artifacts.</description>
<version>1.0</version>
<vendor></vendor>
<configurationParameters>
<configurationParameter>
<name>PatternsActor</name>
<type>String</type>
<multiValued>true</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>Rule</name>
<type>String</type>
<multiValued>true</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
<configurationParameter>
<name>PatternsArea</name>
<type>String</type>
<multiValued>true</multiValued>
<mandatory>true</mandatory>
</configurationParameter>
</configurationParameters>
<configurationParameterSettings>
<nameValuePair>
<name>Rule</name>
<value>
<array>
<string>PSDS-SERPRO</string>
</array>
</value>
</nameValuePair>
<nameValuePair>
<name>PatternsActor</name>
<value>
<array>
<string>\d[.]\d[.]\d[.](?si)(.*?)Descri..o(?si)(.*?)Papel(?si)(.*?)Insumos\s</string>
</array>
</value>
</nameValuePair>
<nameValuePair>
<name>PatternsArea</name>
<value>
<array>
<string>\d\.\d\..Papel\sdos\sAtores(?si)((.*?))\d\..Necessidades\se\sFuncionalidades\s*</string>
</array>
</value>
</nameValuePair>
</configurationParameterSettings>
<typeSystemDescription>
<imports>
<import location="../ItemsTypeSystem.xml"/>
</imports>
<types>
<typeDescription>
<name>org.tom.types.ActorArea</name>
<description></description>
<supertypeName>uima.tcas.Annotation</supertypeName>
<features>
<featureDescription>
<name>rule</name>
<description></description>
<rangeTypeName>uima.cas.String</rangeTypeName>
</featureDescription>
</features>
</typeDescription>
</types>
</typeSystemDescription>
<typePriorities/>
<fsIndexCollection/>
<capabilities>
<capability>
<inputs>
<type allAnnotatorFeatures="true">org.tom.types.ActorArea</type>
</inputs>
<outputs>
<type allAnnotatorFeatures="true">org.tom.types.Actor</type>
</outputs>
<languagesSupported/>
</capability>
</capabilities>
<operationalProperties>
<modifiesCas>true</modifiesCas>
<multipleDeploymentAllowed>true</multipleDeploymentAllowed>
